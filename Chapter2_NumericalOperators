# Numerical Operators

print(39//5)
print(39/5)
print(round(39/5))
print(2**3)
print(pow(2,5))
print(divmod(38,5))
print(pow(2,5,6))
print(round(5/3,3))
print(bin(12))
print(len(str(4*22)))
print(oct(12))

i = int(input("Enter any numerical value"))
print(i)

for countdown in 5,4,3,2,1,"Hey !!!":
    print(countdown)
    
# How to reverse String simply by using slicing

s = "Kanishk Sharan"
print(s,"\n",s[::-1])
